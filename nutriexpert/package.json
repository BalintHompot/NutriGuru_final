{
  "name": "NutriGuru",
  "version": "0.1.0",
  "author": "Balint Hompot",
  "private": true,
  "homepage": "./",
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^13.6.6",
    "electron-builder": "^20.31.2",
    "electron-prebuilt-compile": "^3.0.10",
    "electron-react-devtools": "^0.5.3",
    "typescript": "^3.1.6",
    "wait-on": "^3.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@trendmicro/react-sidenav": "^0.4.5",
    "animated": "^0.2.2",
    "electron-is-dev": "^1.0.1",
    "node-sass": "^4.11.0",
    "react": "^16.6.3",
    "react-addons-css-transition-group": "^15.6.2",
    "react-alert": "^4.0.4",
    "react-dom": "^16.6.3",
    "react-icons": "^3.2.2",
    "react-modal": "^3.8.1",
    "react-multiselect-list": "^0.1.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-sidebar": "^3.0.2",
    "react-sticky": "^6.0.3",
    "react-transition-group": "^1.2.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"BROWSER=none PORT=3000 yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "preelectron-pack": "yarn build",
    "electron-pack": "npx build --x64 --macos --win --c.extraMetadata.main=build/electron.js --publish=never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "extends": null,
    "appId": "com.kp.eu",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "img/icon.png",
      "target": [
        "pkg",
        "dmg",
        "mas"
      ]
    },
    "mas": {
      "entitlements": "assets/entitlements.mas.plist",
      "entitlementsInherit": "assets/entitlements.mas.inherit.plist",
      "provisioningProfile": "assets/embedded.provisionprofile"
    },
    "win": {
      "icon": "img/icon.png",
      "target": "nsis"
    },
    "linux": {
      "icon": "assets/icon4.png",
      "target": [
        "snap",
        "AppImage",
        "deb"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
